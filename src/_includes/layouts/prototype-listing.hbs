{{!-- TODO: This whole thing should be a clickable card --}}

<section class="EP_prototype-listing">
  <a class="EP_prototype-listing__graphic EP_preview" href="{{url url}}">
    <iframe class="EP_preview__iframe" src="{{url url}}"></iframe>
  </a>
  
  <div class="EM_u_stagger-items">
    <h2>{{data.title}}</h2>

    {{#if data.labels}}
      <div>
        {{#each data.labels}}
          <span class="EP_label EP_label--{{this}}">
            {{this}}
          </span>
        {{/each}}
      </div>
    {{/if}}

    {{#if data.notes}}
      {{{markdown data.notes}}}
    {{/if}}
  </div>
</section>